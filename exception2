a=5
b=1

try:
    print("resource open")
    print(a/b)
    p = int(input("enter a number"))
    print(p)
except ZeroDivisionError as e:
    print("you can't divide by zero",e)
except ValueError as v:
    print("not an onteger",v)
except Exception as e:
    print("unknown error")
finally:
    print("resource closed")


