class A:
    def feature1(self):
        print("Feature 1 working")
    def feature2(self):
        print("Feature 2 is working")
class B(A):
    def feature3(self):
        print("Feature 3 working")
    def feature4(self):
        print("Feature 4 is working")
class C(B):#multilevel
    def feature5(self):
        print("Feature 5 working")
class D(A,B):#multiple when B doesnt inherit A
    def feature3(self):
        print("Feature 3 working")
a1=A()
a1.feature1()
a1.feature2()
b1=B()
c1=C()
d1=D()
